/*---- nav ----*/
div.nav{
	height:100px;
	width:1350px;
}
div.nav li {
	float:left;
	background:white;
}
div.nav li.logo-nav{
	background: url(images/logo_s1.png) no-repeat;
	height: 66px;
	width: 121px;
	margin:22px 74px 0 34px;
}
div.nav li.logo-nav a{
	display: block;
	height: 66px;
	width: 121px;
}
div.nav li.nav-item a{
	display: block;
	height:25px;
	line-height: 25px;
	padding: 0 48px 0 48px;
	cursor: pointer;
	text-decoration: none;
	color:black;
	font-size: 12px;
	font-family: 'Lato-Bold';
	letter-spacing: 2px;
}
div.nav li.nav-item{
	border-width: 0 1px 1px 0 ;
	border-style: solid;
	border-color: black;
}
div.nav li.first{
	border-width: 0px 1px 1px 1px ;
	border-style: solid;
	border-color: black;
}
div.nav li.nav-item a:hover{
	background: black;
	color: white
}

div.nav ul{
	background: black;
	width:100%;
}

div.nav li.focus{
	background: black;
}

div.nav li.focus a{
	color:white;
}
